<div class="widget-header">
	{{name}}
</div>
<div class="widget__body">
	<h2 class="widget--island">Please fix me!</h2>
	{{#data.shame}}
		<div class="panel">
			<a class="text-M {{class}}" href="{{url}}">{{text}} total: {{coverageText}}</a>
		</div>
	{{/data.shame}}

	{{#data.jobs}}
		<div class="panel" style="display: none">
			<span class="text-M">{{text}}</span>, total: {{coverageText}}
		</div>
	{{/data.jobs}}
</div>